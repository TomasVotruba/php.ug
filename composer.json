{
    "name": "php_ug/website",
    "description": "The website showing all PHP-Usergroups on the worldmap",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-curl": "*",
        "doctrine/doctrine-module": "^1.0",
        "doctrine/doctrine-orm-module": "^1.0",
        "guzzlehttp/guzzle": "^6.2",
        "monolog/monolog": "^1.9",
        "org_heigl/contact": "^1.0",
        "org_heigl/dateintervalcomparator": "^1.0",
        "org_heigl/geolocation": "^2.0",
        "org_heigl/hybridauth": "^3.0",
        "org_heigl/mailproxy": "^2.0",
        "rwoverdijk/assetmanager": "^1.4",
        "sabre/vobject": "^4.0@dev",
        "willdurand/geocoder": "^2.4",
        "zendframework/zendframework": "^3.0",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
        "zendframework/zendservice-twitter": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "zendframework/zend-developer-tools": "dev-master"
    },
    "autoload": {
        "psr-4": {
           "Phpug\\": "src/module/Phpug/src",
           "UgHealth\\": "src/module/UgHealth/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
           "PhpugTest\\": "tests",
           "UgHealthTest\\": "tests"
        }
    }
}
